function enemy_spawns(dt)
 e_timer+=dt
	if e_before_boss>0 then
		if e_timer>e_delay then
			e_timer=0
			e_before_boss-=1
			local b=0
			local n=level+flr(rnd(2))-1
			local rand1=
					rnd(120-n*10)+10
			//local rand2=rnd(90)+8
			local spread=
				rnd(1)>0.1
			local stagger
				= rnd(1)>0.5 or not spread
				
			local all_bullet
				=rnd(1)<0.2+level/20
			for i=0,n-1 do
				local x=rand1
				local y=-6
				local e_sprite=16
				if stagger then
					y-=i*5
				end
				if spread then
					x+=i*10
				end
				local pat={}
				if all_bullet then
					e_sprite=17
			 	pat[1]=circle
		(2,40,10,true,3)
			 	b=1
				end
				if b==0 and 
					rnd(1) < (level/70) then
					e_sprite=18
					pat[1]=circle_spin
						(0.4,10,4)
					b=1
				elseif b==0 and 
					rnd(1)<(level/100) then
			 	e_sprite=19
			 	pat[1]=spinner
		(0.7,0.1,30,0.1,0,10)
			 	b=1
				else end
				add(enemies,
						basic_enemy(
							x,y,
							x,130,20,
							6,6,8,pat))
				enemies[#enemies].spr.n=e_sprite
		end
		end
 elseif #enemies ==0 then
 	e_timer=0
		if boss==nil then
			boss=boss_enemy
							(60,-4,
							60,20,10,
							8,8,120+(level*10),
							lvl[level])
			boss.e.spr.n=3+level
		else
			boss:update(dt)
			if boss.dead then
				boss=nil
				level+=1
				plr.hp+=1
				if boss_rush then
					e_before_boss=0
				else
					e_before_boss=
						2+level*enemy_waves_per_level
				end
			end
		end
	else end

end
