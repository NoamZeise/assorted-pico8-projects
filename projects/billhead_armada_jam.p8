--helpers--

function lim_abs(val,lim)
	if val>lim then
		return lim
	elseif val<-lim then
		return -lim
	end
	return val
end

function make_rect(x,y,w,h)
	local r={}
	r.x=x
	r.y=y
	r.w=w
	r.h=h
	
	function r:coll(other)
		return 
								self.x<other.x+other.w
								and
								self.x+self.w>other.x
								and
								self.y<other.y+other.h
								and
								self.y+self.h>other.y
	end
	
	function r:draw(col)
		rectfill(
			self.x,
			self.y,
			self.x+self.w-1,
			self.y+self.h-1,
			col)
	end
	
	return r
end

--sprite--

function sprite(x,y,n)
	local s={}
	s.x=x
	s.y=y
	s.n=n
	s.w=1
	s.h=1
	s.flip_x=false
	s.filp_y=false
	
	function s:draw()
		spr(
			self.n,
			self.x,
			self.y,
			1,
			1,
			s.flip_x,
			s.flip_y)
	end
	
	return s
end
