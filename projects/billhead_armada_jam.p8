--enemy--

function basic_enemy
	(x,y,destx,desty,speed,w,h,hp,pats)
	local e={}
	if x<-30 then
		x=-40
	end
	e.rect = make_rect(x,y,w,h)
	e.hp=hp
	e.hpmax=hp
	e.dead=false
	e.dx=0
	e.dy=0
	e.speed=speed
	e.destx=destx
	e.desty=desty
	e.pat=pats
	e.boss=false
	e.out=false
	e.mag=0
	e.immortal=false
	e.spr=sprite(x,y,3)
		
	function e:update(dt)
		local dirx=
			self.destx-self.rect.x
		local diry=
			self.desty-self.rect.y
		self.mag=sqrt(
			dirx*dirx+diry*diry)
		dirx/=self.mag
		diry/=self.mag
		
		self.dx=dirx*self.speed
		self.dy=diry*self.speed
	
		self.rect.x+=self.dx*dt
		self.rect.y+=self.dy*dt
		
		self.spr.x=self.rect.x
		self.spr.y=self.rect.y

		self.dead= self.hp<=0
		self:_bul_collide()
		if self.rect.y > 128 then
			self.hp=0
			self.out=true
		end
		if self.hp > 0 and
				self.rect:coll(plr.rect)then
			if not self.immortal then
				self.hp=0
			end
			plr:dmg()
		end
		for p in all(self.pat)do
			p:shoot(dt,
				self.rect.x+
						self.rect.w/2,
				self.rect.y+
						self.rect.h/2)
		end
	end
	
	function e:draw()
		--self.rect:draw(8)
		self.spr:draw()
	end
	
	function e:_bul_collide()
		for b in all(pbuls) do
			if b.p.life>0 then
				if self.rect:coll(b.rect)then
					self.hp-=1
					b.p.life=0
					sfx(6)
					for i=0,3 do
						emit(
									b.p.x,
									b.p.y,
								b.p.dx*0.2+
									(rnd(50)-25),
							 b.p.dy*0.2+
									(rnd(50)-25),
								0.4,8)
						end
				end
			end
		end
		if self.dead and not self.out then
			if self.boss then
			 score+=50*level
			else
		 	score+=10*level
		 end
		 sfx(4)
			for i=0,10 do
						emit(
									self.rect.x+
									self.rect.w/2,
									self.rect.y+
									self.rect.h/2,
									(rnd(100)-50),
							 	(rnd(100)-50),
									0.4,8)
						end
		end
	end
	
	return e
end

function boss_enemy
										(x,y,destx,desty,speed,w,h,hp,pats)
	local e={} 
	e.e=basic_enemy(x,y,
		destx,desty,speed,w,h,hp,pats)
	e.spr=sprite(1,1,1)
	
	e.dead=false
	e.e.dy=0
	e.e.boss=true
	e.e.spr.n=4
	e.move_delay=7.5
	e.move_timer=0
	e.e.immortal=true

	function e:update(dt)
		self.e:update(dt)
		self.dead=self.e.dead
		self.move_timer+=dt
		
		if self.e.mag>2 then
			self.move_timer=0
		end
		
		if self.move_timer>self.move_delay then
			self.move_timer=0
			self.e.destx=
				flr(
					rnd(120-self.e.rect.w) +8
				)
		end
	end	
	
	function e:draw()
		self.e:draw()
	end
	
	return e
end
