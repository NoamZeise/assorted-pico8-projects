--player--

function player(x,y)
	local p={}
	p.spr=sprite(x,y,1)
	p.dx=0
	p.dy=0
	
	p.ddx=8
	p.ddy=8
	
	p.hp=3
	p.dead=false
	
	--updated locals
	p.mag=0
	p.mvx=0
	p.mvy=0
	p.ds_lim=1.5
	p.rect=make_rect(0,0,0,0)
	p.shoot_timer=0
	p.shoot_left=false
	p.inv_timer=10
	p.last_strobe=0
	--consts
	p.fric=1
	p.ds_max=1.5
	p.ds_slow=0.8
	p.drect=make_rect(3,3,2,2)
	p.bul_speed=100
	p.shoot_delay=0.06
	p.inv_delay=2
	p.inv_strobe=0.2
	
	function p:update(dt)
		self:_controls()
		self:_calc_mom(dt)
		self:_emit()
		self.rect=
			make_rect(
				self.spr.x+self.drect.x,
				self.spr.y+self.drect.y,
				self.drect.w,
				self.drect.h)
		self.shoot_timer+=dt
		
		if self.hp<=0 then
			self.dead=true
		end
		
		for b in all(ebuls) do
			if b.p.life>0 then
				if b.rect:coll(self.rect)then
					b.p.life=0
					self:dmg()
				end
			end
		end
		
		self.inv_timer+=dt
	end
	
	function p:draw()
		if self.inv_timer<
						self.inv_delay then
			if self.inv_timer
			    -self.last_strobe
			    >self.inv_strobe then
				self.last_strobe=self.inv_timer
				if self.spr.n==1 then
					self.spr.n=2
					sfx(2)
				else
					self.spr.n=1
				end
			end
		else
			self.spr.n=1
			self.last_strobe=0
		end

		self.spr:draw()
		if(self.ds_lim==self.ds_slow)then
			self.rect:draw(11)
		end
		for i=1,self.hp do
			rectfill(
					5+i*6,
					122,
					8+i*6,
					125, 
					7)
		end
	end
	
	function p:dmg()
		if(self.inv_timer>self.inv_delay) then
			self.hp-=1
			sfx(1)
			self.inv_timer=0
		end
	end
	
	--private methods
	
	function p:_controls()
		self.mvx=0
		self.mvy=0
	 if btn(0) then
			self.mvx-=1
		end
		if btn(1) then
			self.mvx+=1
		end
		if btn(2) then
			self.mvy-=1
		end
		if btn(3) then
			self.mvy+=1
		end
		if btn(4) and
					self.shoot_delay <
						self.shoot_timer then
			self:_shoot()
			self.shoot_timer=0
		end
		if btn(5) then
			self.ds_lim=self.ds_slow
		else
			self.ds_lim=self.ds_max
		end
	end
	
	function p:_calc_mom(dt)
		self.dx+=self.mvx*self.ddx*dt
		self.dy+=self.mvy*self.ddy*dt
		
		
		self.spr.x+=self.dx
		self.spr.y+=self.dy
		self.dx*=self.fric
		self.dy*=self.fric
		
		self.mag=
			sqrt(self.dx*self.dx
				+self.dy*self.dy)
				
		if(self.mag>self.ds_lim)then
			self.dx=
				(self.dx/self.mag)*self.ds_lim
			self.dy=
				(self.dy/self.mag)*self.ds_lim
		end
		
		if self.spr.x<0 then
			self.spr.x=0
			self.dx=-self.dx
		end
		if self.spr.x>120 then
			self.spr.x=120
			self.dx=-self.dx
		end
		
		if self.spr.y<0 then
			self.spr.y=0
			self.dy
			=-self.dy
		end
		if self.spr.y>120 then
			self.spr.y=120
			self.dy=-self.dy
		end
	end
	
	function p:_emit()
	 if(self.mvx!=0) or
				(self.mvy!=0) then
				sfx(5)
				emit(self.spr.x +
									4
									-self.mvx*4,
									self.spr.y + 
									4
									-self.mvy*4,
								-self.ds_lim
								+ (rnd(20)-10),
							 -self.ds_lim
								+ (rnd(20)-10),
								0.4,7)
		end
	end
	
	function p:_shoot()
		local off=0
		if p.shoot_left then
			sfx(3)
			off=-2
		else
			off=2
		end
		p.shoot_left=not p.shoot_left
		add(
					pbuls,bullet(
							self.rect.x 
								+self.rect.w/2
								+off,
							self.rect.y,
								//+self.rect.h/2,
							0,-p.bul_speed, 2,12)
					)
	end
	
	return p
end
