function spinner
	(sd,spin,speed,sep,rot,num)
	local p={}
	p.shoot_timer=0
	p.shoot_delay=sd
	p.spin=spin
	p.speed=speed
	p.sep=sep
	p.rot=rot
	p.num=num
	
	function p:shoot(dt,x,y)
		self.shoot_timer+=dt
		if(self.shoot_timer>
						self.shoot_delay)then
			self.shoot_timer=0		
			self.rot+=self.spin
			for i=0,self.num-1 do
				local rot=self.rot+i*self.sep
			//rot-=(self.num*self.sep)/2
				add(
					ebuls,bullet(
							x,
							y,
							sin(rot)
							 *self.speed,
							cos(rot)*self.speed,
							10,8)
					)
			end
		end
	end

	return p
end

function circle_spin(sd,speed,num)
	local sep=1.0/num
	return
		spinner
			(sd,0,speed,sep,0,num)
end

function circle
	(sd,speed,num,target,dist)
 local p={}
 p.shoot_timer=0
	p.shoot_delay=sd
	p.speed=speed
	p.tar=target
	p.sep=1.0/num
	p.dist=dist
 
 function p:shoot(dt,x,y)
		self.shoot_timer+=dt
		
		local pvx= plr.spr.x-x
		local pvy= plr.spr.y-y
		local pvm= sqrt(pvx*pvx+pvy*pvy)
		pvx/=pvm
		pvy/=pvm
		if(self.shoot_timer>
						self.shoot_delay)then
			self.shoot_timer=0
			
			angle=0
			while angle<1 do
				angle+=self.sep
				if self.tar then
				local parx=x+sin(angle)*self.dist
				local pary=y+cos(angle)*self.dist
				local pvx= plr.spr.x-parx
				local pvy= plr.spr.y-pary
				local pvm= sqrt(pvx*pvx+pvy*pvy)
				pvx/=pvm
				pvy/=pvm
			 	add(
						ebuls,bullet(
							parx,
							pary,
							pvx*self.speed,
							pvy*self.speed,
							10,8)
					)
				else
				 add(
						ebuls,bullet(
							x+sin(angle)*self.dist,
							y+cos(angle)*self.dist,
							pvx*self.speed,
							pvy*self.speed,
							10,8)
					)
				end
			end
		end
 end
 
	return p
end


