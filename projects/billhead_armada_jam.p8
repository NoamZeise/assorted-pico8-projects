--particles--

function update_ptcl_list
										(dt, list)
	i=1
	while i<=#list do
		list[i]:update(dt)
		if(list[i].dead)then
			del(list,list[i])
			i-=1
		end
		i+=1
	end
end

function emit(x,y,dx,dy,life,col)
	add(
					particles,
					make_particle(
						x,y,dx,dy,life,col
					)
			 )
end

function make_particle
										(x,y,dx,dy,life,col)
	local p={}
	p.x=x
	p.y=y
	p.dx=dx
	p.dy=dy
	p.life=life
	p.col=col
	p.timer=0
	p.dead=false
	
	function p:update(dt)
		self.timer+=dt
		if(self.timer>self.life)then
			self.dead=true
		end
		self.x+=dt*self.dx
		self.y+=dt*self.dy
	end
	
	function p:draw()
		pset(
			self.x,
			self.y,self.col)
	end
	
	return p
end
