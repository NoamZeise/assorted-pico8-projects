--main--

function _init()
	in_menu=true
	
	last_t=time()
	plr=player(64,100)
	particles={}
	pbuls={}
	ebuls={}
	enemies={}
	e_timer=2
	e_delay=4
	boss=nil
	level=1
	lvl={}
	lvl[1]=lvl1()
	lvl[2]=lvl2()
	lvl[3]=lvl3()
	lvl[4]=lvl4()
	lvl[5]=lvl5()
	lvl[6]=lvl6()
	lvl[7]=lvl7()
	lvl[8]=lvl8()
	max_level=#lvl
	continues_used=0
	enemy_waves_per_level=1
	unpressed=false
	e_before_boss=
		2+level*enemy_waves_per_level
	win=false
	score=0
	boss_rush=false
	
	--menu
	selected=1
	lvl_selected=1
	options={}
	options[1]="play"
	options[2]="boss rush"
	options[3]="help"
	options[4]="quit"
	show_controls=false
	
	--end-menu
end

function _update()
 local dt=time()-last_t

if in_menu then
 if show_controls then
 	if btn(4) and btn(5) then
 	 show_controls=false
 	end
 end
 if btnp(0) then
		lvl_selected-=1
	end
	if btnp(1) then
		lvl_selected+=1
	end
	if lvl_selected <1then
		lvl_selected=max_level
	end
	if lvl_selected>max_level then
		lvl_selected=1
	end
	
	if btnp(2) then
		selected-=1
	end
	if btnp(3) then
		selected+=1
	end
	if selected<1 then
		selected=#options
	end
	if selected>#options then
		selected=1
	end
 if btnp(4) then
 	if selected == 1 then
			in_menu=false
			level=lvl_selected
			e_before_boss=
		   2+level*enemy_waves_per_level
		elseif selected==2 then
			in_menu=false
			boss_rush=true
			level=lvl_selected
			e_before_boss=0
		elseif selected==3 then
			show_controls=true
		else
			stop()
		end
	end
else

if plr.dead then
		if not btn(4) and
				not btn(5) then
			unpressed=true
		end
		if unpressed then
			if btnp(4) then
				continues_used+=1
				score=0
				plr=player(64,100)
				plr.inv_timer=0
				ebuls={}
			elseif btnp(5) then
				_init()
			else end
		end
elseif level>max_level then
	if not btn(4) and
				not btn(5) then
			unpressed=true
		end
		if unpressed then
			if btnp(4) then
				_init()
			elseif btnp(5) then
			 stop()
			else end
		end
else
	unpressed=false
	plr:update(dt)
	update_ptcl_list(dt,enemies)
	update_ptcl_list(dt,particles)
	update_ptcl_list(dt,pbuls)
	update_ptcl_list(dt,ebuls)
	
	enemy_spawns(dt)
end

end	--end menu else
	last_t=time()
end

