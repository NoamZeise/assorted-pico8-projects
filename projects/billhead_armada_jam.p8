--bullets--

function bullet
										(x,y,dx,dy,life,col)
	local b={}
	b.rect=make_rect(x,y,1,1)
	b.p=
		make_particle
			(x,y,dx,dy,life,col)
	b.dead=false

	function b:update(dt)
		self.p:update(dt)
		self.rect.x=self.p.x
		self.rect.y=self.p.y
		self.dead=self.p.dead
		
		if self.rect.y<=-10 then
			self.dead=true
		end
		if self.rect.y>=130 then
			self.dead=true
		end
		if self.rect.x<=-10 then
			self.dead=true
		end
		if self.rect.x>=130 then
			self.dead=true
		end	
	end
	
	function b:draw()
		self.p:draw()
	end
	
	return b
end
