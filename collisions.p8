function camerascroll(speed)
	cam.x+=(speed * (time() - cam.time))
	cam.time=time()
end

function camerafollow(a, bound)
	offset=a.x - ((128-a.w)/2)
	if(cam.x -offset > 10) then
		cam.x=offset + bound
	elseif(cam.x-offset<-10)then
		cam.x=offset - bound
	end
	if(cam.x<0)then cam.x=0 end
	if(cam.x>896)then cam.x=896 end
end

function frameupdate(a)
	if(a.lastx>a.x) then
		a.frame=17
	elseif a.lastx<a.x then
		a.frame=16
	end
	a.lastx=a.x
end



function cloudrender(speed)
 cloudx+=(time()-cloudupdate)*speed	
	cloudupdate=time() 
 local i=0
	while true do
		if(-cloudx)+(45*8*i) > (-45*8) then
			map(0,16, (-cloudx)+(45*8*i),0,45,16)
		end
		
		i+=1
		if(-cloudx)+(45*8*i) > (1016+45*8) then
			break
		end
	end
end

function cityrender()
 local i=0
 while true do
		map(46,17,i*200,70,25,4)
		i+=1
		if i*200 > 1016 then break end
	end
end

function mountainrender(speed)
 local i=0
 while true do
		 	map(46,21,i*200+(cam.x*speed),40,25,7)
		i+=1
		if i*200 > 1016 then break end
	end
end

function cloudbackdrop(offsetx,offsety)
		map(71,16,(cam.x+offsetx),40+offsety,9,4)
end

function backgroundrender()
 cls(12)
	rectfill(cam.x,100,128+cam.x,128,10)
	rectfill(cam.x,110,128+cam.x,128,2)
	rectfill(cam.x,120,128+cam.x,128,1)
	cloudbackdrop(0,0)
	cloudbackdrop(40,10)
	cloudbackdrop(60,0)
	cloudbackdrop(50,15)
	cloudbackdrop(100,5)
	mountainrender(0.5)
	cityrender()
	cloudrender(2)
end
