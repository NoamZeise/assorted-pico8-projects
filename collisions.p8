function makeplayer(x,y)
 a={}
	a.x=x
	a.y=y
	a.w=8
	a.h=8
	
	a.dx=0
	a.ddy=10 --gravity
	a.dy=0
	
	a.fr=0.80
	a.bounce=0.1
	a.grounded=false
	a.lastupdate=time()
	a.lastgorunded=time()
	
	a.lastx=0
	a.frame=16
	return a
end

function control(a)
	if(btn(0)) then
	 a.dx=-2
	elseif(btn(1))then
	 a.dx=2
	elseif(a.grounded) then
	 a.dx=a.dx*a.fr
	else a.dx=a.dx*a.fr*0.95
	end
	if(btnp(4)and a.grounded)then
	 a.dy=-5
	end
end


-->8
function collide(x,y,d)
	apix=mget(flr(x)/8,flr(y)/8)
	return fget(apix,d)
end

function removecollisions(a,d,orient)
	if orient==2 and sgn(a.dy)==1 then
		for i=2,a.w-3 do
			while(collide(a.x+i,a.y+a.h-1,d))do
		 		a.y-=0.1
		 		a.grounded=true
		 		a.lastgrounded=time()
     a.dy=-a.dy*a.bounce
			end
		end
	elseif orient==3 then
		for i=2,a.w-3 do
			while(collide(a.x+i,a.y,d))do
		 	a.y+=0.1
				a.dy=-a.dy*a.bounce
			end
		end
		
	elseif orient==1 then
		for i=0, a.h-1 do
			while(collide(a.x,a.y+i,d))do
				a.x+=0.1
				a.dx=-a.dx*a.bounce
			end
		end
	elseif(orient==0)then
		for i=0, a.h-1 do
			while(collide(a.x+a.w-1,a.y+i,d))do
				a.x-=0.1
				a.dx=-a.dx*a.bounce
			end
		end
	end
end

function collision(a)
	removecollisions(a,1,3)
	removecollisions(a,1,2)
	
	removecollisions(a,2,1)
	removecollisions(a,2,0)
	
	while(a.x>1016.2) do
		a.x-=0.1
		win=true
	end
	while(a.x<0) do
		a.x+=0.1
 end
 if(a.y>122) then
  gameover=true
 end
end


function physics(a)
	if(a.grounded)and a.dy!=0 and (time()-a.lastgrounded)>0.1 then
		a.grounded=false
	end
	a.dy+=a.ddy*(time()-a.lastupdate)
	a.y+=a.dy
	a.x+=a.dx
	a.lastupdate=time()
end

