player={}
cam={}
cam.x=0
cam.y=0
cam.time=time()
gameover=false
win=false
gametime=0
gameovertime=0
t=nil


cloudx=0
cloudupdate=time()
function _init()
	player=makeplayer(5,40)
	gametime=time()
	t=nil
end

function _update()
 if not gameover and not win then
	control(player)
	physics(player)
	collision(player)
	frameupdate(player)
	camerafollow(player,10)
	camera(cam.x,cam.y)
	end
end

function _draw()
	backgroundrender()
	map(0,0)
	spr(player.frame,player.x, player.y)
	--print(player.frame, 10+cam.x, 10)
	if(gameover) then
		if(gameovertime==0)then
			gameovertime=time()
		end
		camera(0, 0)
		cls(2)
		print("gameover",45,60,7)
		print((player.x/1016)*100 .. "% completed", 10, 10,7)
		print("press ðŸ…¾ï¸ to restart",25,80,7)
		if(btnp(4))then
		 gameover=false
		 _init()
		end
	end
	if(win)then
	if t==nil then
	t=time()-gametime
	end
	camera(0, 0)
	cls(0)
	print("you won!",45,60)
	print("it took "..t.." seconds", 25, 70)
	print("press ðŸ…¾ï¸ to restart",25,80)
	if(btnp(4))then
		 win=false
		 _init()
		end
	end
end
