function lvl
	(x,y,w,h,px,py,pl,pk,ms,ems,fuel)
	
	level={}
	level.x=x
	level.y=y
	level.w=w
	level.h=h
	
	make_cam(0,0)
	
	p=make_player(px, py)
	p.fuel = fuel
	
	cam:update(0)
	
	planets={}
	for plan in all(pl) do
		add(planets,
			make_planet(
				plan[1],plan[2],plan[3])
				)
	end
	
	pickups={}
	for pkup in all(pk) do
		add(pickups,
					make_fuel(pkup[1],pkup[2])
					)
	end
	
	msgs={}
	for msg in all(ms) do
		add(msgs, msg)
	end
	
	shown_end=false
	end_msgs={}
	for msg in all(ems) do
		add(end_msgs,msg)
	end
	
end

function make_cam(x,y)
	cam={}
	cam.x=x
	cam.y=y
	cam.w=128
	cam.h=128
	
	function cam:correct(x,w,lx,lw)
		if -x<lx then
				return -lx
		end
		if -x>lx+lw-w+1 then
			return -(lx+lw-w+1)
		end
		return x
	end
	
	function cam:update(dt)
		self.x=
			-flr(p.circ.x)+self.w/2
		self.y=
			-flr(p.circ.y)+self.h/2
			
		self.x=self:correct(
										self.x,
										self.w,
										level.x,
										level.w)
		self.y=self:correct(
										self.y,
										self.h,
										level.y,
										level.h)
	end
	
	function contains(x,y)
		return 
			x>self.x and
			x<self.x+self.w and 
			y>self.y and 
			y<self.y+self.h
	end
	
end


