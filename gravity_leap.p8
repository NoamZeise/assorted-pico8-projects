function next_lvl()
	currentlvl+=1
	if(currentlvl>maxlevel)then
		gamedone=true
		currentlvl=maxlevel
	end
	load_level()
end

function load_level()

	levels[currentlvl]()
	planets[count(planets)].last=true
	
	particles={}
	
	bg=gen_bg()
	
	stars={}
	local n=(level.w+level.h*5)
	starnum=n
	for i=1,n do
		local sp=16
	 if i>n*0.9 then
	 	sp=17
	 end
	 if i>n*0.95 then
	 	sp=18
	 end
	 if i>n*0.995 then
	 	sp=19
	 end
		add(
			stars,
			make_star(
				rnd(level.w*2)+level.x,
				rnd(level.h*2)+level.y,
				sp))
	end
end

