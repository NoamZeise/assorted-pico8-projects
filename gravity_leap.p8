--main--
pixs={}
function _init()
--vars global by default
	last=time()
	init_levels()
	load_level()
end

function _update()
	dt=time()-last
	if p.lvlcomplete and not
	shown_end then
		shown_end=true
		for msg in all(end_msgs) do
			add(msgs,msg)
		end
	end
	if #msgs>0 then
	 msgs[1]:update(dt)
	 if msgs[1].dead then
	 	del(msgs,msgs[1])
	 end
	else
		update_game()
	end
	last=time()
end

function _draw()
	draw_game()
	if #msgs>0 then
	 msgs[1]:draw()
	end
	if gamedone then
		cls(2)
		print("thanks for playing!"
		,10,30,7)
	end
end

function update_game()
	if(p.lvlcomplete)next_lvl()
	if(p.fuel<0)load_level()
	
	p:update(dt)
	planet_update()
	local i=1
	while i<=#particles do
		particles[i]:update(dt)
		if(particles[i].dead)then
			del(particles,particles[i])
			i-=1
		end
		i+=1
	end
	
	if 
			p.circ.x-p.circ.r<level.x or
			p.circ.x+p.circ.r
				>level.x+level.w or
			p.circ.y-p.circ.r<level.y or
			p.circ.y+p.circ.r
				>level.y+level.h then
			
		p.circ=circ_copy(p.last_c)
		p.dv.x=-p.dv.x*0.2
		p.dv.y=-p.dv.y*0.2
	end
	
	local i=1
	while i<=#pickups do
		if pickups[i]:pcollide()then
			p.fuel+=70
			if(p.fuel>p.fuelmax)then
				p.fuel=p.fuelmax
			end
			del(pickups,pickups[i])
			i-=1
		end
		i+=1
	end
	cam:update(dt)
end

function draw_game()
	cls(0)
	bg:draw()
	for star in all(stars)do
		star:draw()
	end
	for part in all(particles) do
		part:draw()
	end
	
	for pkup in all(pickups)do
		pkup:draw()
	end
	
	p:draw()
	
	for planet in all(planets) do
		planet:draw()
	end
	draw_fuel()
end
