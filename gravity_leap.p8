--properties--

function make_circle(x,y,r)
	local c={}
	c.x=x
	c.y=y
	c.r=r
	
	function c:draw(col)
		circfill(
			self.x+cam.x,
			self.y+cam.y,
			self.r,col) 
	end
	
	return c
end

function circ_copy(c)
	local nc=make_circle(c.x,c.y,c.r)
	return nc	
end

function circ_dist(c1,c2)
	local dist=
							(c1.x-c2.x)*(c1.x-c2.x)
	dist+=(c1.y-c2.y)*(c1.y-c2.y)
	return sqrt(dist)
end

function make_vect(x,y)
	local v={}
	v.x=x
	v.y=y
	return v
end

function copy_vect(v)
	local nv=make_vect(v.x,v.y)
	return nv
end

function rect_collide(
	x1,y1,w1,h1,x2,y2,w2,h2)
	return x1<x2+w2 and
								x1+w1>x2 and
								y1<y2+h2 and
								y1+h1>y2
end

