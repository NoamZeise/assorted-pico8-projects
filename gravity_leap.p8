function make_fuel(x,y)
	local f={}
	f.x=x
	f.y=y
	f.w=6
	f.h=8
	
	function f:draw()
		spr(2,self.x+cam.x,
								self.y+cam.y)
	end
	
	function f:pcollide()
		return rect_collide
			(self.x,self.y,self.w,self.h,
			p.circ.x-p.circ.r,
			p.circ.y-p.circ.r,
			p.circ.r*2,
			p.circ.r*2)
	end
	
	return f
end
