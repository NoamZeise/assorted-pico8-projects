function emit(x,y,dx,dy,life,col)
	add(particles,
		make_particle(x,y,
				make_vect(dx,dy),life,col))
end

function make_particle
										(x,y,vec,life,col)
	local p={}
	p.x=x
	p.y=y
	p.v=vec
	p.life=life
	p.col=col
	p.timer=0
	p.dead=false
	
	function p:update(dt)
		self.timer+=dt
		if(self.timer>self.life)then
			self.dead=true
		end
		self.x+=dt*self.v.x
		self.y+=dt*self.v.y
	end
	
	function p:draw()
		pset(
			self.x+cam.x,
			self.y+cam.y,self.col)
	end
	
	return p
end

function make_star(x,y,sp)
	local s={}
	s.x=x
	s.y=y
	s.par=rnd(10)+0.5
	s.spr=sp
	s.flipy=rnd(1)<0.5
	s.flipx=rnd(1)<0.5
	function s:draw()
			spr(self.spr,
				self.x+(cam.x/self.par),
				self.y+(cam.y/self.par),
				1,1,self.flipx,self.flipy
				)	
	end
	
	return s
end

function make_point(x,y)
	local s={}
	s.x=x
	s.y=y
	return s
end

function gen_bg()
	local b={}
	b.pixs={}
	b.plan={}
	for i=0,100 do
	 add(b.pixs,
	 make_point(rnd(128),rnd(128)))
	end
	for i=0,5 do
		add(b.plan,
		 make_circle(
		 rnd(128),rnd(128),rnd(10)))
	end
	
 function b:draw()
		for pix in all(self.pixs) do
	 	pset(pix.x,pix.y,1)
		end
		for c in all(self.plan) do
			 circfill(c.x,c.y,c.r,1)
		end
 end
	
	return b
end
