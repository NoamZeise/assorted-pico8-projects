function make_player(x,y)
--local shadows global p
	local p = {}
	
	p.circ = make_circle(x,y,3)
	p.last_c = p.circ
	
	p.speed=1
	p.maxspeed=20
	p.dv=make_vect(0,0)
	p.grav=make_vect(0,0)
	
	p.grounded=false
	p.ungtimer=0.3
	p.ungdelay=0.3
	p.groundvec=make_vect(0,0)
	p.boostdir=make_vect(0,0)
	
	p.fuel=250.0
	p.fuelmax=250.0
	p.boostfueluse=15.0
	p.launchfueluse=50.0
	
	p.lvlcomplete=false
	
	function p:draw()
		line(
			self.circ.x+cam.x,
			self.circ.y+cam.y,
			(self.circ.x+cam.x)-
			(self.circ.r*p.boostdir.x*1.2),
			(self.circ.y+cam.y)-
			(self.circ.r*p.boostdir.y*1.2)
			,7)
		spr(1,
			self.circ.x+cam.x-self.circ.r,
			self.circ.y+cam.y-self.circ.r)
	end
	
	function p:update(dt)
		self.last_c=circ_copy(p.circ)
		self.boostdir.x=0
		self.boostdir.y=0
		if btn(4) and 
		self.ungtimer<self.ungdelay then
			self.dv.x+=10*self.groundvec.x
			self.dv.y+=10*self.groundvec.y
		 sfx(2)
		 self.fuel-=dt*self.launchfueluse
		 for i=1,10 do
		 	local col=8
				local rval=flr(rnd(10))
				if(rval==1)then
					col=10
				elseif rval < 3 then
					col=9
				end
		 	emit(self.circ.x-
									--self.circ.r*
									self.groundvec.x,
								self.circ.y-
									--self.circ.r*
									self.groundvec.y,
								-30*self.groundvec.x*
										self.speed*0.5
								+ (rnd(70)-45),
								-30*self.groundvec.y*
										self.speed*1
								+ (rnd(70)-45)*0.5,
								0.2,col)
			end
		elseif(not p.grounded) then
			if btn(0) then
				self.dv.x-=self.speed
				self.boostdir.x-=1
			end
			if btn(1) then
				self.dv.x+=self.speed
				self.boostdir.x+=1
			end
			if btn(2) then
		 	self.dv.y-=self.speed
		 	self.boostdir.y-=1
			end
			if btn(3) then
		 	self.dv.y+=self.speed
		 	self.boostdir.y+=1
			end
		end
		
		self.dv.x+=self.grav.x
		self.dv.y+=self.grav.y
		
		if(self.dv.x > p.maxspeed)then
			self.dv.x-=p.speed
		end
		if(self.dv.x < -p.maxspeed)then
			self.dv.x+=p.speed
		end
		if(self.dv.y > p.maxspeed)then
			self.dv.y-=p.speed
		end
		if(self.dv.y < -p.maxspeed)then
			self.dv.y+=p.speed
		end
				
		self.circ.x+=dt*self.dv.x
		self.circ.y+=dt*self.dv.y
		
		if(self.boostdir.x!=0) or
				(self.boostdir.y!=0) then
				sfx(1)
				self.fuel-=dt*self.boostfueluse
				local col=10
				local rval=flr(rnd(10))
				if(rval<3)then
					col=9
				elseif rval==9 then
					col=8
				end
				emit(self.circ.x-
									self.circ.r*
									self.boostdir.x,
								self.circ.y-
									self.circ.r*
									self.boostdir.y,
								-30*self.boostdir.x*self.speed
								+ (rnd(30)-15),
								-30*self.boostdir.y*self.speed
								+ (rnd(30)-15),
								0.2,col)
		end
		if(self.ungrimer==0)then
			self.dv.x=0
			self.dv.y=0
		end
		self.ungtimer+=dt
	end
	
	return p
end

