function make_planet(x,y,r)
	local p={}
	p.circ = make_circle(x,y,r)
	p.far=false
	p.scheme=colour_schemes[
						flr(
						rnd(#colour_schemes-1)
						)+1]
	p.dec={}
	--add large decorations
	for i=1,
		flr(rnd(p.circ.r*0.5))+2 do
	 local rn=rnd(1)
		local dx= p.circ.x
				+cos(rn)*p.circ.r
		local dy=
			p.circ.y
				+sin(rn)*p.circ.r
		local len=
			rnd(p.circ.r/1.5)
		dx-=cos(rn)*len-0.5
		dy-=sin(rn)*len-0.5
		add(p.dec,
						make_circle(
						dx,
						dy,
						len
						))
	end
	for i=1,flr(rnd(p.circ.r))+10 do
	 local rn=rnd(1)
		local dx= p.circ.x
				+cos(rn)*p.circ.r
		local dy=
			p.circ.y
				+sin(rn)*p.circ.r
		local rl=3
			--flr(rnd(2))
		local len=
			rnd((p.circ.r)/1.5)+rl
		dx-=cos(rn)*len-0.5
		dy-=sin(rn)*len-0.5
		add(p.dec,
						make_circle(
						dx,
						dy,
						rl
						))
	end
	
	function p:draw()
		if self.last then
			self.scheme=colour_schemes[#colour_schemes]
		end
		self.circ:draw(self.scheme[1])
		for d in all(self.dec) do
			d:draw(self.scheme[2])
		end
		--[[circ(self.circ.x+cam.x,
						self.circ.y+cam.y,
		self.circ.r,0)]]--
	end
	
	return p
end

function planet_update()
	p.grav.x=0
	p.grav.y=0
	p.grounded=false
	for planet in all(planets) do
		predist=
			abs(planet.circ.x-p.circ.x)+
			abs(planet.circ.y-p.circ.y)
				
		if(predist>180)then
				planet.far=true
		elseif predist>100 then
				planet.far=false
		end
		
		if not planet.far then
			dist=
				circ_dist(p.circ,planet.circ)
			local pvec=make_vect(0,0)
			pvec.x=p.circ.x-planet.circ.x
			pvec.y=p.circ.y-planet.circ.y
		
			local g= 1 *
				(p.circ.r*planet.circ.r)
						/(dist*dist*1)
			if g > 0 then
			p.grav.x-=pvec.x*g
			p.grav.y-=pvec.y*g
			if dist < p.circ.r+planet.circ.r then
				p.circ=circ_copy(p.last_c)
				p.grounded=true
				p.ungtimer=0
				p.groundvec=copy_vect(pvec)
				p.groundvec.x/=dist
				p.groundvec.y/=dist
				p.dv.x=0
				p.dv.y=0
				if(planet.last)then
					p.lvlcomplete=true
				end
			end
			end
		end
	end
end
