function menuselect()
	if btnp(2) then
		m.current = m.current-1
		sfx(5)
	elseif btnp(3) then
	 m.current = m.current+1
	 sfx(5)
	elseif btnp(4) then
	 m.select = true
	 sfx(1)
	end
	
	if m.current > 3 then
	 m.current = 1
	elseif m.current < 1 then
	 m.current = 3
	end
end


